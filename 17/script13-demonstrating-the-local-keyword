# demonstrating the local keyword

function func1 {
	local temp=$[ $value + 5 ]
	result=$[ $temp * 2 ]
}

temp=4
value=6

func1

if [ $temp -gt $value ]
then
	echo "temp is larger"
else
	echo "temps is smaller"
fi
